{
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/Users/YOUR_USERNAME/Desktop",  // Replace with your actual path
        "/Users/YOUR_USERNAME/Downloads" // Replace with your actual path
      ]
    },
    "weather": {
      "command": "node",
      "args": [
        "/path/to/your/weather-server/build/index.js"  // Replace with the actual path
      ]
    }
  }
}
